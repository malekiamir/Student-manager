<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<StackPane prefWidth="600" prefHeight="600" xmlns:fx="http://javafx.com/fxml" 
           fx:controller="manager.student.MainViewController">
    <children>
        <StackPane>
            <children>
                <Label fx:id="label" text="Undefined" alignment="TOP_CENTER"/>
                <StackPane prefHeight="-1.0" prefWidth="-1.0">
                    <children>
                        <Button id="backButton" fx:id="backButton" 
                                alignment="CENTER" defaultButton="true" 
                                mnemonicParsing="false" text="بازگشت" 
                                StackPane.alignment="BOTTOM_CENTER" />
                    </children>
                </StackPane>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="30.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
